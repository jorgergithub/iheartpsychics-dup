  <section class="user user-signin-element">
    <div class="user-container container_12">
      <div class="user-signup">
        <h1>Sign Up or Connect With</h1>

        <ul class="signup-links">
          <li class="signup-link">
            <%= link_to(image_tag("twitter.png", alt: "Twitter"), "#") %>
          </li>

          <li class="signup-link">
            <%= link_to(image_tag("facebook.png", alt: "Facebook"), "#") %>
          </li>

          <li class="signup-link">
            <%= link_to(image_tag("google+.png", alt: "Google+"), "#") %>
          </li>
        </ul>
      </div>

      <div class="user-signin">
        <h1>Sign Up or Connect With</h1>

        <%= form_for("user", url: user_session_path, html: { class: "signin-form" }) do |f| %>
          <div class="signin-inputs">
            <div class="signin-input">
              <%= f.text_field :login, placeholder: "Username" %>
            </div>

            <div class="signin-input">
              <%= f.password_field :password, placeholder: "Password" %>
            </div>

            <%= submit_tag "Sign In" %>
          </div>

          <p class="signin-actions">
            <a href="#" class="signin-actions-forgot">Forgot Username or Password?</a>
            <%= f.check_box :remember_me %>
            <label for="user_remember_me" class="signin-actions-remember">Remember Me</label>
          </p>
        <% end %>

        <%= link_to(image_tag("user-close.png", alt: "Close"), "#", class: "user-close") %>
      </div>
    </div>
  </section>

